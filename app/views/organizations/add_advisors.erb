<%= render partial: 'shared/errors', :locals => {:object => @organization}%>
<div class="col-md-4 col-lg-4">
  <%= form_for @organization, :url => { :action => :add_advisors_create } do |f| %>
          <table class="table">
            <tr>
              <th>Advisor ID</th>
              <th>Verified</th>
              <th></th>
            </tr>
            <%= f.fields_for :organization_advisors do |builder| %>
                <%= render('organizations/add_advisors_form', f: builder) %>
            <% end %>
          </table>
          <%= add_advisor 'Add Advisor', f, :organization_advisors %>
      <div class="form-group">
        <%= f.submit 'Save Investment Organization', class: 'btn btn-success' %>
      </div>
  <% end %>
</div>